  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "build",
    "target": "es5",
    "module": "commonjs",
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
